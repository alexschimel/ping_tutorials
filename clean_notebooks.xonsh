#!/bin/xonsh
# Clean the notebook cache to save memory online
# Tutorial notebooks are pushed to git lfs!

import os

pytest --nbmake .

for r,d,f in os.walk('.'):
	for file in f:
		if file.endswith('.ipynb'):
			ofi = r+'/'+file
			jupyter nbconvert --ClearOutputPreprocessor.enabled=True --inplace @(ofi)
